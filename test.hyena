fn write(data:&u8) void {
    asm {
        "mov rsi, [rdi+8]"
        "mov rdx, [rdi +0]"
        "mov rdi, 1"
        "mov rax, 1"
        "syscall"
    }
}


fn main() void {
    defer {
        defer write("defer block\n");

        defer {
            defer write("defer inner block\n");
        }
    }
    defer write("write first\n");
}