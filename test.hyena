fn write(data:&u8) void {
    asm {
        "mov rsi, [rdi+8]"
        "mov rdx, [rdi +0]"
        "mov rdi, 1"
        "mov rax, 1"
        "syscall"
    }
}


fn test(a:bool) void {
    if(a) {
        write("test: ok\n");
    } else {
        write("test: fail\n");
    }
}

fn test_eql(a:bool, b:bool) void {
    if(a == b) {
        write("test: ok\n");
    } else {
        write("test: fail\n");
    }
}