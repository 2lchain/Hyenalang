var total = 0;
var passed = 0;
var failed = 0;

fn write(data:&u8) void {
    asm {
        "mov rsi, [rdi+8]"
        "mov rdx, [rdi +0]"
        "mov rdi, 1"
        "mov rax, 1"
        "syscall"
    }
}


fn test(a:bool) void {
    total = total + 1;
    if(a) {
        write("test: ok\n");
        passed = passed + 1;
    } else {
        write("test: fail\n");
        failed = failed + 1;
    }
}

fn test_eql(a:bool, b:bool) void {
    total = total + 1;
    if(a eq b) {
        write("test: ok\n");
        passed = passed + 1;
    } else {
        failed = failed + 1;
        write("test: fail\n");
    }
}

